<?xml version="1.0" encoding="UTF-8"?>
<scenario>
    <world>
        <region name="India">
            <supplysector name="refining">
                <subsector name="biomass liquids">
                    <interpolation-rule apply-to="share-weight"
                        from-year="2010" to-year="2100">
                        <from-value>0.017</from-value>
                        <to-value>0.25</to-value>
                        <interpolation-function name="linear"/>
                    </interpolation-rule>
                    <interpolation-rule apply-to="share-weight"
                        from-year="2095" to-year="2100">
                        <from-value>0.25</from-value>
                        <to-value>0.25</to-value>
                        <interpolation-function name="fixed"/>
                    </interpolation-rule>
                </subsector>
            </supplysector>
        </region>
    </world>
</scenario>
